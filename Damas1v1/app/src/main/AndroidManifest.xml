<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="activity.damas1v1">
    <uses-permission android:name="android.permission.VIBRATE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/titulo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="AllowBackup,GoogleAppIndexingWarning">

        <activity android:name=".Principal.PantallaPrincipal"
            android:screenOrientation="portrait"
            tools:ignore="InnerclassSeparator">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Principal.SeleccionDeJugadores"
            android:screenOrientation="portrait"
            tools:ignore="InnerclassSeparator" />
        <activity android:name=".Principal.SeleccionDeTablero"
            android:screenOrientation="portrait"
            tools:ignore="InnerclassSeparator" />

        <activity android:name=".Principal.PantallaDeAyuda"
            android:screenOrientation="portrait"
            tools:ignore="InnerclassSeparator" />

        <activity android:name=".Jugadores.AdministradorDeJugadores"
            android:screenOrientation="portrait"
            tools:ignore="InnerclassSeparator"
            />
        <activity android:name=".Jugadores.EdicionDeJugador"
            android:screenOrientation="portrait"
            tools:ignore="InnerclassSeparator" />

        <activity android:name=".LogicaDelJuego.Juego"
            android:screenOrientation="portrait"
            tools:ignore="InnerclassSeparator" />

        <activity android:name=".FinesDelJuego.FinEnVictoria"
            android:screenOrientation="portrait"
            tools:ignore="InnerclassSeparator" />
        <activity android:name=".FinesDelJuego.FinEnEmpate"
            android:screenOrientation="portrait"
            tools:ignore="InnerclassSeparator" />

    </application>

</manifest>